{
  "name": "devbuff",
  "version": "1.0.0",
  "description": "Devbuff",
  "author": "DevBuff, Alexander Katkov <katkov.alexander@yahoo.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "precommit": "npm run lint",
    "storybook": "npx nuxt storybook",
    "build-storybook": "npx nuxt storybook build",
    "chromatic": "npx chromatic",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
