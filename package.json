    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "lint-staged": "^10.4.0",
    "node-sass": "5.0.0",
    "nuxt-build-optimisations": "^1.0.2",
    "postcss": "^7.0.35",
    "prettier": "^2.1.2",
    "sass-loader": "10.1.0",
    "stylelint": "^13.7.2",
