    "@fortawesome/vue-fontawesome": "2.0.2",
    "@nuxtjs/axios": "5.12.5",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/pwa": "3.3.4",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "axios": "0.21.1",
    "color": "3.1.3",
    "cookie-universal-nuxt": "2.1.4",
    "dedent": "0.7.0",
    "highlight.js": "10.5.0",
